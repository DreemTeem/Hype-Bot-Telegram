{
  "name": "hype-bot-telegram",
  "version": "0.0.0",
  "description": "hype-bot-telegram",
  "license": "MIT",
  "repository": "",
  "author": "",
  "keywords": [
    ""
  ],
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "build": "npm run clean && npm run lint && echo Using TypeScript && tsc --version && tsc --pretty",
    "test": "npm run build && mocha --compilers ts:ts-node/register --recursive 'test/**/*-spec.ts'",
    "coverage": "nyc --include='src/**/*.ts' --reporter=text --reporter=html --reporter=lcov mocha --require ts-node/register --compilers ts:ts-node/register --recursive 'test/**/*-spec.ts'",
    "watch": "npm run build -- --watch",
    "watch:test": "npm run test -- --watch",
    "start": "node dist/index.js",
    "dev": "npm run build && npm start"
  },
  "dependencies": {
    "firebase-admin": "^8.9.2",
    "gosugamers-api": "^0.1.2",
    "node-telegram-bot-api": "^0.40.0",
    "request": "^2.86.0",
    "scryfall-sdk": "^1.1.0",
    "tslint": "^6.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.9",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.1",
    "@types/node-telegram-bot-api": "^0.40.2",
    "@types/sinon": "^7.5.1",
    "chai": "^4.2.0",
    "coveralls": "^3.0.9",
    "mocha": "^7.0.1",
    "nyc": "^15.0.0",
    "rimraf": "^3.0.2",
    "sinon": "^8.1.1",
    "ts-node": "^8.6.2",
    "tslint": "^5.3.2",
    "typescript": "^3.7.5"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "nyc": {
    "include": [
      "src/**/*"
    ],
    "exclude": [
      "lib"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [],
    "all": true
  }
}
